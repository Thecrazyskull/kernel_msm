			qcom,pc-sf-lut-row-entries = <100 200 300 400 500>;
			qcom,pc-sf-lut-percent =
					<100 90 80 70 60 50 40 30 20 10>;
			qcom,pc-sf-lut-sf = <100 100 100 100 100>,
					<100 100 100 100 100>,
					<100 100 100 100 100>,
