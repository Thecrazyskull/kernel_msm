		qcom,calibration-type = "ratiometric";
		qcom,scale-function = <2>;
		qcom,hw-settle-time = <2>;
		qcom,fast-avg-setup = <0>;
	};

	chan@17 {
		label = "pa_therm1";
		reg = <0x17>;
		qcom,decimation = <0>;
		qcom,pre-div-channel-scaling = <0>;
		qcom,calibration-type = "ratiometric";
		qcom,scale-function = <2>;
		qcom,hw-settle-time = <2>;
		qcom,fast-avg-setup = <0>;
	};
};

&pm8226_lvs1 {
	regulator-always-on;
};

&i2c_2 {
	status = "ok";

	ct406@39 {
		compatible = "ams,ct406";
		reg = <0x39>;
		interrupt-parent = <&msmgpio>;
		interrupts = <65 0x2>;
		gpios = <&msmgpio 65 0>; /* IRQ */
		vdd-supply = <&pm8226_l19>;
		ams,prox-samples-for-noise-floor = <0x05>;
		ams,prox-covered-offset = <0x0096>;
		ams,prox-uncovered-offset = <0x006e>;
		ams,prox-recalibrate-offset = <0x0046>;
		ams,prox-offset = <0x00>;
		ams,prox-pulse-count = <0x04>;
		ams,ink_type = <0x01>;
	};

	ak8963@0c {
		compatible = "akm,akm8963";
		reg = <0x0c>;
		vdd-supply = <&pm8226_l19>;
		interrupt-parent = <&msmgpio>;
		interrupts = <66 0x2>;
		akm,gpio-irq = <&msmgpio 66 0>;
		akm,gpio-rst = <&msmgpio 62 0>;
		akm,outbit = <1>;
		akm,layout = <6>;
	};
};

&i2c_3 {
	status = "ok";

	tmp108: tmp108@48 {
		compatible = "ti,tmp108";
		reg = <0x48>;
		gpios = <&msmgpio 13 0>;
	};


	tpa6165xx@40 {
		compatible = "ti,tpa6165";
		reg = <0x40>;
		interrupt-parent = <&msmgpio>;
		interrupts = <4 0x2>;
		gpios = <&msmgpio 4 0x0>;
		hs_det_vdd-supply = <&pm8226_l6>;
		hs_det_micvdd-supply = <&pm8226_l21>;
	};

	tps65132@3e {
		compatible = "ti,tps65132";
		reg = <0x3e>;
		gpios = <&msmgpio 31 0x0>, <&msmgpio 33 0x0>;
		active-dis;
		bias-delay-us = <2000>;
		tps65132: regulator {
			regulator-name = "tps65132";
			regulator-min-microvolt = <4000000>;
			regulator-max-microvolt = <6000000>;
			startup-delay-us = <10000>;
		};
	};
};

&slim_msm {
	tapan_codec {
		qcom,cdc-micbias1-ext-cap;
	};
};

&mdss_dsi0 {
	qcom,supply-names = "vddio", "vdda";
	qcom,supply-min-voltage-level = <1800000 1200000>;
	qcom,supply-max-voltage-level = <1800000 1200000>;
	qcom,supply-peak-current = <100000 100000>;
};

&cci {
	qcom,hw-thigh = <20>;
	qcom,hw-tlow = <28>;
	qcom,hw-tsu-sto = <6>;
	qcom,hw-tsu-sta = <7>;
	qcom,hw-thd-dat = <13>;
	qcom,hw-thd-sta = <11>;
	qcom,hw-tbuf = <25>;
	qcom,hw-scl-stretch-en = <0>;
	qcom,hw-trdhld = <6>;
	qcom,hw-tsp = <3>;
};
