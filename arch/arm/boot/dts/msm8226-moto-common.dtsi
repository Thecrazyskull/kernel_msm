/include/ "msm8226-camera-sensor-falcon.dtsi"

&soc {
	factory_support {
		compatible = "mmi,factory-support-msm8960";

		gpios = <&msmgpio 5 0x2>; /* GPIOF_DIR_OUT | GPIOF_INIT_HIGH */
		gpio-names = "factory_kill_disable";
	};

	serial@f991f000 {
		status = "ok";
	};

	ram_console {
		compatible = "android,ram-console";
		status = "ok";

		android,ram-buffer-start = <0x3eae0000>;
		android,ram-buffer-size = <0x20000>;
	};

	bootinfo {
		compatible = "android,bootinfo";

		android,bootinfo-buffer-start = <0x3eb00000>;
		android,bootinfo-buffer-size = <0x800>;
	};

	i2c_5: i2c@f9927000 {
		synaptics_dsx_i2c@20 {
			compatible = "synaptics,synaptics_dsx_i2c";
			reg = <0x20>;
			interrupt-parent = <&msmgpio>;
			interrupts = <17 0>;
			gpios = <&msmgpio 17 0>, /* IRQ */
				<&msmgpio 16 0>; /* RESET */
			touch_vdd-supply = <&pm8226_l28>;
			synaptics,normal-mode = <1>;
			synaptics,gpio-config;
		};

		cyttsp3_i2c@3b {
			compatible = "cyttsp3,i2c";
			reg = <0x3b>;
			interrupt-parent = <&msmgpio>;
			interrupts = <17 0x0>;
			vdd-supply = <&pm8226_l19>;
			vcc_i2c-supply = <&pm8226_lvs1>;
		};
	};

	gpio_keys {
		compatible = "gpio-keys";
		input-name = "gpio-keys";

		camera_focus {
			label = "camera_focus";
			gpios = <&msmgpio 108 0x1>;
			linux,input-type = <1>;
			linux,code = <0x210>;
			gpio-key,wakeup;
			debounce-interval = <15>;
		};

		camera_snapshot {
			label = "camera_snapshot";
			gpios = <&msmgpio 107 0x1>;
			linux,input-type = <1>;
			linux,code = <0x2fe>;
			gpio-key,wakeup;
			debounce-interval = <15>;
		};

		vol_up {
			label = "volume_up";
			gpios = <&msmgpio 106 0x1>;
			linux,input-type = <1>;
			linux,code = <115>;
			gpio-key,wakeup;
			debounce-interval = <15>;
		};
	};

	spi@f9923000 {
		ethernet-switch@3 {
			compatible = "micrel,ks8851";
			reg = <3>;
			interrupt-parent = <&msmgpio>;
			interrupts = <0 115 0>;
			spi-max-frequency = <4800000>;
			rst-gpio = <&msmgpio 114 0>;
			vdd-io-supply = <&pm8226_lvs1>;
			vdd-phy-supply = <&pm8226_lvs1>;
		};
	};

	sound {
		qcom,audio-routing =
			"RX_BIAS", "MCLK",
			"LDO_H", "MCLK",
			"SPK_OUT", "MCLK",
			"SPK_OUT", "EXT_VDD_SPKR",
			"AMIC1", "MIC BIAS1 External",
			"MIC BIAS1 External", "Handset Mic",
			"AMIC2", "MIC BIAS2 External",
			"MIC BIAS2 External", "Headset Mic",
			"AMIC3", "MIC BIAS1 External",
			"MIC BIAS1 External", "ANCRight Headset Mic",
			"AMIC4", "MIC BIAS2 External",
			"MIC BIAS2 External", "ANCLeft Headset Mic";

		qcom,cdc-mclk-gpios = <&pm8226_gpios 1 0>;
		qcom,cdc-vdd-spkr-gpios = <&pm8226_gpios 2 0>;
	};

	sound-9302 {
		qcom,audio-routing =
			"RX_BIAS", "MCLK",
			"LDO_H", "MCLK",
			"SPK_OUT", "MCLK",
			"SPK_OUT", "EXT_VDD_SPKR",
			"AMIC1", "MIC BIAS1 External",
			"MIC BIAS1 External", "Handset Mic",
			"AMIC2", "MIC BIAS2 External",
			"MIC BIAS2 External", "Headset Mic",
			"AMIC3", "MIC BIAS1 External",
			"MIC BIAS1 External", "ANCRight Headset Mic",
			"AMIC4", "MIC BIAS2 External",
			"MIC BIAS2 External", "ANCLeft Headset Mic";

		qcom,cdc-mclk-gpios = <&pm8226_gpios 1 0>;
		qcom,cdc-vdd-spkr-gpios = <&pm8226_gpios 2 0>;
	};

	usb@f9a55000 {
		qcom,hsusb-otg-phy-init-seq = <0x34 0x82 0x2f 0x81 0xffffffff>;
		qcom,hsusb-otg-mode = <3>;
		vbus_otg-supply = <&pm8226_chg_otg>;
		otg,id_flt_gpio = <&msmgpio 111 0x0>;
		otg,id_gnd_gpio = <&msmgpio 112 0x0>;
		id_flt_active_high = <1>;
		id_gnd_active_high = <1>;
	};

	alsa_to_h2w {
		compatible = "mmi,alsa-to-h2w";
	};

	qcom,mdss_wb_panel {
		status = "disabled";
	};

	vddio_disp_vreg: vddio_disp_vreg {
		compatible = "regulator-fixed";
		regulator-name = "vddio_disp";
		gpio = <&msmgpio 34 0x0>;
		parent-supply = <&pm8226_l8>;
		startup-delay-us = <300>;
		enable-active-high;
	};

	hob_ram@8300000 {
		compatible = "mmi,hob_ram";
		reg = <0x8300000 0x40000>,
		      <0x8340000 0x2000>;
	};
	usr_reset_warning {
		compatible = "mmi,usr-reset-warning";
		gpios = <&msmgpio 69 0x0>;
	};

	qcom,ion {
		qcom,ion-heap@23 { /* OTHER PIL HEAP */
			qcom,memory-fixed = <0x06400000 0x1F00000>;
		};
	};

	bms_tbls: qcom,batt-tbls {
		compatible = "qcom,batt-tbls";

		mmi,batt-tbl-one {
			qcom,batt-tbl-name = "mmi,batt-tbl-ed30-temp";
			qcom,batt-tbl-id = <4005 4389>;

			qcom,batt-tbl-fcc = <1950>;
			qcom,batt-tbl-default-rbatt-mohm = <193>;
			qcom,batt-tbl-rbatt-capacitive-mohm = <50>;
			qcom,batt-tbl-flat-ocv-threshold-uv = <3800000>;

			qcom,fcc-temp-lut-cols = <4>;
			qcom,fcc-temp-lut-x = <0xFFFFFFF6 0 23 60>;
			qcom,fcc-temp-lut-y = <1950 1950 1950 1950>;

			qcom,fcc-sf-lut-cols = <6>;
			qcom,fcc-sf-lut-x = <1 100 200 300 400 500>;
			qcom,fcc-sf-lut-y = <100 97 95 94 93 91>;

			qcom,pc-sf-lut-rows = <10>;
