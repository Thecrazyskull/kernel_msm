/include/ "msm8226-moto-common.dtsi"
/include/ "dsi-panel-mot-cmi-720p-video.dtsi"

/ {
	model = "Motorola QC-MSM8226 Falcon";
	compatible = "qcom,msm8226-falcon",
		     "qcom,msm8226-moto",
		     "qcom,msm8226";

	qcom,mdss_dsi_mot_cmi_720p_video {
		status = "ok";
	};
};

&i2c_5 {
	cyttsp3_i2c@3b {
		status = "disabled";
	};
};

/* Supplies necessary to support mot-cmi-720p */
&mdss_dsi0 {
	qcom,supply-names = "vddio", "vdda", "lcdbias";
	qcom,supply-type = "regulator", "regulator", "regulator";
	qcom,supply-min-voltage-level = <1800000 1200000 5400000>;
	qcom,supply-max-voltage-level = <1800000 1200000 5400000>;
	qcom,supply-peak-current = <100000 100000 100000>;
};
