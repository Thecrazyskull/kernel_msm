/dts-v1/;

/include/ "msm8610-v2.dtsi"
/include/ "msm8610-moto-common.dtsi"

/ {
	/* HWREV: 0x0101 0x8100
	 *        ^^^^^^ ^^^^^^
	 *        Rev1.1 P1(P0C)
	 */
	qcom,msm-id = <147 0x41 0x01018100>;
};

&i2c {
	qcom,camera@6c {
		gpios = <&msmgpio 13 0>,
			<&msmgpio 21 0>,
			<&msmgpio 20 0>,
			<&msmgpio 14 0>;
		/delete-property/ qcom,gpio-pwren2;
		qcom,gpio-req-tbl-num = <0 1 2 3>;
		qcom,gpio-req-tbl-flags = <1 0 0 0>;
		qcom,gpio-req-tbl-label = "CAMIF_MCLK",
			 "CAM_RESET1",
			 "CAM_STANDBY",
			 "CAM_PWREN";
	};
};

&pm8110_l20 {
	/delete-property/ regulator-always-on;
};
